void check_conflict() // the logic is not well planned
{    
    if( route[data_lane][data_direction] != 1){
       route[data_lane][data_direction] = 1;
        conflict = false;
    }
    else{
        conflict = true;
    }
}



void send_data(car veh)
{
    data_lane = veh.lane;
    data_direction = veh.direction;
    data_veh_id = veh.id; 
}

